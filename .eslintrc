{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "indent": [1, "tab",
      {
        "SwitchCase": 1,
        "VariableDeclarator": 1
      }
    ],
    "no-tabs": 0,
    "react/jsx-indent": [1, "tab"],
    "react/jsx-indent-props": [1, "tab"],
    "react/no-array-index-key": 0,
    "react/jsx-tag-spacing": 0,
    "react/jsx-filename-extension": 0,
  }
}